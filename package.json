{
  "name": "backbone-webpack4-jest-starter",
  "version": "1.0.0",
  "description": "A starter project for Backbone.js with ES6, Webpack4, Handlebars, Bootstrap and Jest libraries.",
  "scripts": {
    "build": "node node_modules/webpack/bin/webpack --config webpack.config.js",
    "start": "node node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.dev.config.js --progress --colors --hot --inline",
    "test": "jest"
  },
  "keywords": [
    "backbone.js",
    "ES6",
    "webpack",
    "handlebars",
    "lodash",
    "jest"
  ],
  "author": "Erik Yuzwa",
  "license": "MIT",
  "dependencies": {
    "backbone": "^1.4.0",
    "bootstrap": "^4.4.1",
    "handlebars": "^4.5.3",
    "jquery": "^3.1.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.0",
    "file-loader": "^5.0.2",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "postcss-loader": "^3.0.0",
    "robotstxt-webpack-plugin": "^7.0.0",
    "sass": "^1.24.0",
    "sass-loader": "^8.0.0",
    "source-map-support": "^0.5.16",
    "style-loader": "^1.1.1",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-node-externals": "^1.7.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "html"
    ],
    "verbose": true
  }
}
